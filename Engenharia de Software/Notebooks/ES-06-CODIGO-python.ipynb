    "# CENÁRIO de composição funcional\n",
    "def g(p, q) :\n",
    "  if len(p) == len(q) :\n",
    "    return [p[i]-q[i] for i in range(len(p))]\n",
    "  raise Exception('_|_')\n",
    "\n",
    "def s(v) :\n",
    "  return sum([(vi*vi) for vi in v])\n",
    "# uso da soma de listas\n",
    "y = [1,2,3]; z = [2,3,4]\n",
    "w = s(g(y,z))\n",
    "w"
